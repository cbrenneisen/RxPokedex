    pod 'RxRealm'
    pod 'SDWebImage'

  target 'RxPokedexTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RxPokedexUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
